{
  "name": "btv-meetup",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/sync-mcpb.mjs && next dev -p 8000",
    "dev:ngrok": "node scripts/dev-ngrok.mjs",
    "build": "node scripts/sync-mcpb.mjs && next build",
    "start": "next start -p 8000",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "supabase:reset": "supabase db reset --local && psql 'postgresql://postgres:postgres@127.0.0.1:54322/postgres' -f supabase/schema.sql",
    "seed:local": "node scripts/seed-local.mjs"
  },
  "dependencies": {
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.0",
    "next": "15.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "vercel": "^50.22.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.6",
    "typescript": "^5.7.3"
  }
}
